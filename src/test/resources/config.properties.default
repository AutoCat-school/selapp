wait.timeout=10
use.remote.web.driver=false
use.remote.web.url=http://localhost:4444

extent.report.path=target/extent/ExtentReport.html
extent.report.theme.dark=true

verify.result.in.report=true
verify.report.is.true=true
verify.screenshot.on.pass=true
